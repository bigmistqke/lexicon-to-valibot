{
  "name": "@bigmistqke/lexicon-to-valibot",
  "version": "0.1.0",
  "description": "Convert AT Protocol Lexicon schemas to Valibot validators",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "bumpp && pnpm build"
  },
  "keywords": ["lexicon", "valibot", "atproto", "bluesky", "validation"],
  "license": "MIT",
  "peerDependencies": {
    "valibot": ">=1.0.0"
  },
  "devDependencies": {
    "@atproto/lexicon": "^0.4.0",
    "bumpp": "^9.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "valibot": "^1.0.0",
    "vitest": "^2.0.0"
  }
}
